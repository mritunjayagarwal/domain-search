<body>
    <nav class="navbar navbar-expand-lg py-3 m-0" data-aos="fade-down" data-aos-delay="800">
        <!-- Container wrapper -->
        <div class="container">
            <!-- Navbar brand -->
            <a class="navbar-brand me-2" style="color: #000; margin-top: -11px" href="/">
                <img src="/img/logo.png" class="img-fluid" style="width: 100px; height: auto;" />
            </a>

            <!-- Toggle button -->
            <button class="navbar-toggler text-white" type="button" data-toggle="collapse" data-target="#navbarText"
                aria-controls="navbarText" aria-expanded="false" aria-label="Toggle navigation">
                <img src="/img/ham.png" style="width: 30px" alt="">
            </button>

            <!-- Collapsible wrapper -->
            <div class="collapse navbar-collapse text-white" id="navbarText">
                <ul class="navbar-nav ml-auto mb-2 mb-lg-0 mt-1">
                    <li class="nav-item nav-link-item">
                        <a href="#" class="px-3 pt-2 btn-link">
                            Home
                        </a>
                    </li>
                    <li class="nav-item nav-link-item">
                        <a href="/files/weblayer.pdf" target="_blank"
                            class="px-3 pt-2 btn-link">
                            Whitepaper
                        </a>
                    </li>
                    <li class="nav-item nav-link-item">
                        <a href="https://t.me/WebLayerErc"
                            class="px-3 pt-2 btn-link">
                            Community
                        </a>
                    </li>
                    <% if(!user){ %>
                        <li class="nav-item nav-link-item">
                            <a href="/login" class="px-3 pt-2 btn-link">
                                Sign In
                            </a>
                        </li>
                        <% } %>
                            <li class="nav-item">
                                <% if(user){ %>
                                    <a href="/domains/list">
                                        <button type="button" class="px-3 btn main-btn main-nav-btn" data-toggle="modal"
                                            data-target="#exampleModal">
                                            Dashboard
                                        </button>
                                    </a>
                                    <% }else{ %>
                                        <a href="/signup">
                                            <button type="button" class="px-3 btn pt-2 main-btn main-nav-btn"
                                                data-toggle="modal" data-target="#exampleModal">
                                                Signup
                                            </button>
                                        </a>
                                        <% } %>
                            </li>
                </ul>
            </div>
            <!-- Collapsible wrapper -->
        </div>
        </div>
        <!-- Container wrapper -->
    </nav>
    <main>
        <section class="hero-section">
            <div class="container text-center">
                <h1 class="hero-head" data-aos="fade-up" data-aos-duration="1000">Unlock the Future of Domain and Cloud
                    Hosting with Cryptocurrencies</h1>
                <p class="hero-desc" data-aos="fade-up" data-aos-duration="1000" data-aos-delay="500">Web Layer
                    simplifies domain registration and cloud hosting with secure,
                    cryptocurrency
                    transactions, offering a seamless, blockchain-enhanced web management experience.</p>
                <p class="hero-desc">Contact Address - TBA</p>
                <div class="d-flex justify-content-center mt-3">
                    <div>
                        <a href="/signup" style = "color: #fff">
                            <button class="hero-btn-left">Sign Up</button>
                        </a>
                    </div>
                    <div>
                        <a href="/login" style = "color: #fff">
                            <button class="hero-btn-right">Sign In</button>
                        </a>
                    </div>
                </div>
            </div>
        </section>
        <section class="domain-search-section">
            <div class="container">
                <div class="domain-search-wrapper">
                    <form action="/domain/search" method="POST" class="domain-search-form">
                        <div class="row">
                            <div class="col-lg-7">
                                <input type="text" name="domainName" placeholder="Type your domain address..." required>
                            </div>
                            <div class="col-lg-2">
                                <select name="tld" required>
                                    <option value="com">.com</option>
                                    <option value="net">.net</option>
                                    <option value="org">.org</option>
                                    <option value="dev">.dev</option>
                                    <option value="io">.io</option>
                                    <option value="co">.co</option>
                                </select>
                            </div>
                            <div class="col-lg-3">
                                <button type="Submit" class="domain-search-btn btn-block">Search</button>
                            </div>
                        </div>
                    </form>
                </div>
                <% if(domain){ %>
                    <!-- <div class="d-flex mt-4 bg-dark pt-3">
                        <p style="flex: 1">
                            <%= domain.Domain %>
                        </p>
                        <form action="/domain/buy" method="POST" style="flex: 1">
                            <input type="text" name="domainName" id="" value="<%= domain.Domain %>" hidden>
                            <button type="submit">Buy This Domain</button>
                        </form>
                    </div> -->
                    <h1 class="mt-5 domain-result-title">Search Result</h1>
                    <div class="domain-result-wrapper">
                        <div class="row">
                            <div class="col-lg-3">
                                <p class="mt-3">
                                    <%= domain.Domain %>
                                </p>
                            </div>
                            <div class="col-lg-2">
                                <p class="mt-3">
                                    <%= domain.Available==='true' && domain.IsPremiumName==='false' ? 'Available'
                                        : 'Not Available' %>
                                </p>
                            </div>
                            <div class="col-lg-2">
                                <p class="mt-3">
                                    $<%= domains.find(price=> price.TLD == domain.Domain.split('.')[1]).Price %>
                                </p>
                            </div>
                            <div class="col-lg-2">
                                <select name="tld" class="domain-result-year-select" required>
                                    <option>1 Year</option>
                                </select>
                            </div>
                            <div class="col-lg-3">
                                <% if(domain.Available==='true' && domain.IsPremiumName==='false' ){ %>
                                    <form action="/checkout" method="POST">
                                        <input type="text" name="domainName" value="<%= domain.Domain %>" hidden>
                                        <button type="submit" class="domain-search-btn btn-block">
                                            Buy Now
                                        </button>
                                    </form>
                                    <% }else{ %>
                                        <button type="button" class="domain-search-btn btn-block" disabled>
                                            Unavailable
                                        </button>
                                        <% } %>
                            </div>
                        </div>
                    </div>
                    <% } %>
            </div>
        </section>
        <section class="pricing">
            <div class="container">
                <div class="text-center">
                    <p class="text-center"><span class="golden-pill">TLD List</span></p>
                    <h1 class="section-heading mt-3">Domains Supported</h1>
                    <p class="section-para">Discover an expansive selection of domain names tailored to meet the diverse
                        needs of individuals and businesses alike.</p>
                    <div class="row mt-5">
                        <% domains.forEach(domain=> { %>
                            <div class="col-lg-2 col-6" data-aos="fade-up" data-aos-duration="1000">
                                <h class="section-heading">.<%= domain.TLD %>
                                        </h1>
                                        <p>$<%= domain.Price %>
                                        </p>
                            </div>
                            <% }) %>
                    </div>
                </div>
            </div>
        </section>
